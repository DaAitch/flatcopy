    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "phantomjs-prebuilt": "^2.1.13",
    "ts-loader": "^0.9.1",
    "tsc": "^1.20150623.0",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
